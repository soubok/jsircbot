function() {

	this.InitModule = function() {

		var _module = this;

		this.AddMessageListener( 'PRIVMSG', function( from, command, to, msg ) {

			var nick = from.substring( from.indexOf('!')+1 );

			if ( msg == '!!dump' ) {

				//_module.Send( 'PRIVMSG '+nick+' :'+dumpData(_module.data) );
				Print(dumpData(_module.data), '\n');
			}

			if ( msg == '!!flood' ) {

				for ( var i = 0; i<100; i++ )
				  _module.Send( 'PRIVMSG '+nick+' :XxxxxxxxXxxxxxxxXxxxxxxxXxxxx '+i );
			}

			if ( msg == '!!quit' ) {

				_module.api.Quit('bye...');
			}

		});

		addDataListener( _module.data.channel['#jslibs'].names['soubok'], function(info) {
		
			_module.Send( 'PRIVMSG soubok :hi! ('+info+')' );
//			_module.Send( 'PRIVMSG soubok :'+_module.api.Ctcp('VERSION') );
			
		});
	}
}

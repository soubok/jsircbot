	function StartServer() {
	
		serverConnection = new TCPServer( 80, undefined, 1 );
		serverConnection.OnIncoming = function(connection) {

			if ( count++ > maxConnections ) {
			
				connection.Close();
			} else {
			
				connection.OnData = function(buf) {
					
					Print( buf );
				
				}
			}
		}
	}
	
	
